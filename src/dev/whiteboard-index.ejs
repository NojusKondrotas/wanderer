<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>wanderer</title>
    <link rel="stylesheet" href="../main/titlebar-style.css">
    <link rel="stylesheet" href="../main/styles.css">
</head>
<body>
    <p id="window-component-id" class="im-whiteboard-text"></p>

    <%- include('titlebars/whiteboard/lock') %>

    <%- include('configs-menu') %>

    <div class="context-menu-container cm-logic" id="general-context-menu">
        <button class="option-control-style text-small option-control-layout gcm" id="gcm-new-whiteboard">New Whiteboard</button>
        <button class="option-control-style text-small option-control-layout gcm" id="gcm-new-note">New Note</button>
        <button class="option-control-style text-small option-control-layout gcm" id="gcm-new-notepad">New Notepad</button>
        <button class="option-control-style text-small option-control-layout gcm" id="gcm-new-connection">New Connection</button>
        <button class="option-control-style text-small option-control-layout gcm" id="gcm-paste">Paste</button>
    </div>
    <div class="context-menu-container cm-logic" id="element-context-menu">
        <button class="option-control-style text-small option-control-layout npcm" id="npwcm-copy">Copy</button>
        <button class="option-control-style text-small option-control-layout npcm" id="npwcm-delete">Delete</button>
        <button class="option-control-style text-small option-control-layout npcm" id="npwcm-cut">Cut</button>
        <button class="option-control-style text-small option-control-layout npcm" id="npwcm-style">Style</button>
        <button class="option-control-style text-small option-control-layout npcm" id="npwcm-connect">Connect</button>
        <button class="option-control-style text-small option-control-layout npcm" id="npwcm-disconnect">Disconnect</button>
    </div>
    <div class="context-menu-container cm-logic" id="path-context-menu">
        <button class="option-control-style text-small option-control-layout acm" id="acm-delete">Remove</button>
        <button class="option-control-style text-small option-control-layout acm" id="acm-style">Style</button>
        <button class="option-control-style text-small option-control-layout acm" id="acm-connect">Connect</button>
        <button class="option-control-style text-small option-control-layout acm" id="acm-disconnect">Disconnect</button>
    </div>
    <div class="context-menu-container cm-logic" id="titlebar-context-menu">
        <button class="option-control-style text-small option-control-layout fullscreen-window tcm" id="tcm-fullscreen-window"><img src="../../res/fullscreen-window.png"></button>
        <button class="option-control-style text-small option-control-layout maximize-window tcm" id="tcm-maximize-window"><img src="../../res/maximize-window.png"></button>
        <button class="option-control-style text-small option-control-layout minimize-window tcm" id="tcm-minimize-window"><img src="../../res/minimize-window.png"></button>
        <button class="option-control-style text-small option-control-layout close-window tcm" id="tcm-close-window"><img src="../../res/close-window.png"></button>
        <button class="option-control-style text-small option-control-layout global-config tcm" id="tcm-global-config-menu"><img src="../../res/global-configuration.png"></button>
    </div>

    <div id="parent-whiteboard">
    </div>

    <button class="path-end pathconnector-logic" id="path-end-0"></button>
    <button class="path-end pathconnector-logic" id="path-end-1"></button>

    <div id="keybind-guide">
        <button id="keybind-guide-close"><img id="kbn-gd-cl-img" src="../../res/config-close.png"></button>
        <div id="keybind-container">
            <p class="keybind-text im-whiteboard-text">Ctrl + 1: titlebar menu</p>
            <p class="keybind-text im-whiteboard-text">Ctrl + 2: tabs</p>
            <p class="keybind-text im-whiteboard-text">Ctrl + X: shut down</p>
            <p class="keybind-text im-whiteboard-text">Shift + W: drag window</p>
            <p class="keybind-text im-whiteboard-text">Shift + S: resize window</p>
        </div>
    </div>

    <script src="../runtime/numerics.js"></script>
    <script src="../runtime/clipboard.js"></script>
    <script src="../runtime/states-handler.js"></script>
    <script src="../ui/window-component-id-handler.js"></script>
    <script src="../ui/parent-whiteboard-handler.js"></script>
    <script src="../ui/titlebars/titlebar.js"></script>
    <script src="../ui/titlebars/whiteboard/lock.js"></script>
    <script src="../instantiable-components/note-toolbar-handler.js"></script>
    <script src="../ui/context-menus/context-menu-handler.js"></script>
    <script src="../ui/context-menus/general-cm.js"></script>
    <script src="../ui/context-menus/element-cm.js"></script>
    <script src="../ui/context-menus/path-cm.js"></script>
    <script src="../ui/context-menus/titlebar-cm.js"></script>
    <script src="../ui/tabs-menu-handler.js"></script>
    <script src="../instantiable-components/hierarchy-handler.js"></script>
    <script src="../instantiable-components/component-handler.js"></script>
    <script src="../instantiable-components/path-connection-handler.js"></script>
    <script src="../instantiable-components/note.js"></script>
    <script src="../instantiable-components/notepad.js"></script>
    <script src="../instantiable-components/whiteboard.js"></script>
    <script src="../instantiable-components/path.js"></script>
    <script src="../ui/keybind-guide.js"></script>
    <script src="../ui/keybinds.js"></script>
    <script src="../ui/zoom-whiteboard.js"></script>
    <script src="../ui/positioning/mouse-drag-calc.js"></script>
    <script src="../ui/positioning/window-positioning.js"></script>
    <script src="../ui/positioning/whiteboard-positioning.js"></script>
    <script src="../ui/configs-menu/config-abstract.js"></script>
    <script src="../ui/configs-menu/config-info-tag.js"></script>
    <script src="../ui/configs-menu/configs.js"></script>
    <script src="../main/whiteboard-renderer.js"></script>
</body>
</html>